{
  "scripts": {
    "type-check": "npm run type-check:shared && npm run type-check:backend && npm run type-check:frontend",
    "type-check:shared": "tsc --noEmit --project libs/shared/tsconfig.json",
    "type-check:backend": "tsc --noEmit --project backend/tsconfig.app.json",
    "type-check:frontend": "tsc --noEmit --project frontend/tsconfig.app.json",
    "lint-fix": "npm run lint-fix:shared && npm run lint-fix:backend && npm run lint-fix:frontend",
    "lint-fix:shared": "nx lint shared --fix",
    "lint-fix:backend": "nx lint backend --fix", 
    "lint-fix:frontend": "nx lint frontend --fix",
    "quality-check": "npm run type-check && npm run lint-fix"
